<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/rounded_layout_orange"
    android:orientation="horizontal"
    android:padding="@dimen/extra_small_padding"
    android:baselineAligned="false">

    <LinearLayout
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:layout_marginRight="@dimen/extra_small_margin"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:orientation="horizontal">

            <org.wizbots.labtab.customview.TextViewCustom
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="Sku:"
                android:textColor="@color/purple"
                android:textSize="16sp"
                app:fontName="@string/font_pt_sans_narrow_bold" />

            <org.wizbots.labtab.customview.TextViewCustom
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="3052"
                android:textColor="@color/white"
                android:textSize="14sp"
                android:id="@+id/tv_lab_sku" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:orientation="horizontal">

            <org.wizbots.labtab.customview.TextViewCustom
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="Availability:"
                android:textColor="@color/purple"
                android:textSize="16sp"
                app:fontName="@string/font_pt_sans_narrow_bold" />

            <org.wizbots.labtab.customview.TextViewCustom
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="@string/yes"
                android:textColor="@color/white"
                android:textSize="14sp"
                android:id="@+id/tv_availability" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:orientation="horizontal">

            <org.wizbots.labtab.customview.TextViewCustom
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="Name:"
                android:textColor="@color/purple"
                android:textSize="16sp"
                app:fontName="@string/font_pt_sans_narrow_bold" />

            <org.wizbots.labtab.customview.TextViewCustom
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="Late Pickup"
                android:textColor="@color/white"
                android:textSize="14sp"
                android:id="@+id/tv_name" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:orientation="horizontal">

            <org.wizbots.labtab.customview.TextViewCustom
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="Location:"
                android:textColor="@color/purple"
                android:textSize="16sp"
                app:fontName="@string/font_pt_sans_narrow_bold" />

            <org.wizbots.labtab.customview.TextViewCustom
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="Palo Verde ELementary"
                android:textColor="@color/white"
                android:textSize="14sp"
                android:id="@+id/tv_location" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="120dp"
        android:layout_height="match_parent"
        android:layout_marginRight="@dimen/extra_small_margin"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:orientation="horizontal">

            <org.wizbots.labtab.customview.TextViewCustom
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="Category:"
                android:textColor="@color/purple"
                android:textSize="16sp"
                app:fontName="@string/font_pt_sans_narrow_bold" />

            <org.wizbots.labtab.customview.TextViewCustom
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="Summer Camps"
                android:textColor="@color/white"
                android:textSize="14sp"
                android:id="@+id/tv_category" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:orientation="horizontal">

            <org.wizbots.labtab.customview.TextViewCustom
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="Room:"
                android:textColor="@color/purple"
                android:textSize="16sp"
                app:fontName="@string/font_pt_sans_narrow_bold" />

            <org.wizbots.labtab.customview.TextViewCustom
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="YTBD"
                android:textColor="@color/white"
                android:textSize="14sp"
                android:id="@+id/tv_room" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:orientation="horizontal">

            <org.wizbots.labtab.customview.TextViewCustom
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="Grades:"
                android:textColor="@color/purple"
                android:textSize="16sp"
                app:fontName="@string/font_pt_sans_narrow_bold" />

            <org.wizbots.labtab.customview.TextViewCustom
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="2,3,4,5,6,7,8"
                android:textColor="@color/white"
                android:textSize="14sp"
                android:id="@+id/tv_grades" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:orientation="horizontal">

            <org.wizbots.labtab.customview.TextViewCustom
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="Price:"
                android:textColor="@color/purple"
                android:textSize="16sp"
                app:fontName="@string/font_pt_sans_narrow_bold" />

            <org.wizbots.labtab.customview.TextViewCustom
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="308.0"
                android:textColor="@color/white"
                android:textSize="14sp"
                android:id="@+id/tv_price" />
        </LinearLayout>

    </LinearLayout>

</LinearLayout>